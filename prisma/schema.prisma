generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String @default(uuid())
  name     String
  email    String @unique
  password String

  @@id([id])
}

model Post {
  id      String @default(uuid())
  title   String
  content String

  @@id([id])
}
